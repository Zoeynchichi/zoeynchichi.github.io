{
  "name": "zoeynchichi.github.io",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://zoeynchichi.github.io",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "next lint",
    "lint-fix": "lint-staged",
